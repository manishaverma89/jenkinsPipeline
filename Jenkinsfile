pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
         stage('Build') {
            steps {
                echo 'Building'
            }
        }
         stage('deploy') {
            steps {
                echo 'Deploying'
            }
        }
         stage('Test') {
            steps {
                echo 'testing'
            }
        }
         stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
    }
}

